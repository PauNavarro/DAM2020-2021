package ironman;

public class Variables {

	public static void main(String[] args) {

		// Las variables que se pueden utilizar en java son :
		int a;
		long b;
		double c;
		float d;
		String e;
		boolean f;

		// Para transformalos en constantes solamente añadimos un "final" delante de la variable:
		
		final int a1;
		final long b1;
		final double c1;
	}
}